
URxvt.geometry:             80x25
URxvt.saveLines:            10000

!! Fonts
URxvt.allow_bold:           false
URxvt.font: xft:Inconsolata\ for\ Powerline:size=11, xft:IPAGothic:size=10

!! Scrollbar
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false

!! Perl extensions
URxvt.perl-ext-common:      default,url-select,keyboard-select,selection-to-clipboard,resize-font
! url-select 
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.url-select.launcher:          /usr/bin/xdg-open
URxvt.url-select.underline:          true
! keyboard-select
URxvt.keysym.M-Escape:           perl:keyboard-select:activate
! resize-font
URxvt.resize-font.smaller:          C-q
URxvt.resize-font.bigger:           C-w
! URxvt.matcher.button:       1
! URxvt.urlLauncher:          google-chrome
! URxvt.keysym.M-t:           perl:tabbedex:new_tab
! URxvt.tabbed.no-tabbedex-keys: true


! ! Cursor
URxvt.cursorUnderline:      false


! Color
urxvt*depth: 32
!urxvt*background: rgba:0000/0000/0200/c800
urxvt*background: [80]#000000
urxvt*foreground:           #c6c6c6
